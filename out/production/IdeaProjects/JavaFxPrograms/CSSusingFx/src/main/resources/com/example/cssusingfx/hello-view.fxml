<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Hyperlink?>
<?import com.sun.javafx.scene.control.WebColorField?>
<GridPane alignment="CENTER" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.cssusingfx.HelloController" vgap="10" hgap="10" stylesheets="@style.css" fx:id="gridPane">
<Button text="Open.." GridPane.rowIndex="0" GridPane.columnIndex="0" onAction="#fileChooser"/>
  <Button text="Button Two" GridPane.rowIndex="0" GridPane.columnIndex="1" />
  <Button text="Button Three" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
  <Button text="Button Four" GridPane.rowIndex="0" GridPane.columnIndex="3" fx:id="buttonFour"/>
  <Button text="Button Five" GridPane.rowIndex="0" GridPane.columnIndex="4" />
  <RadioButton text="Take Choice" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
  <Label text="JavaFx File" fx:id="label" GridPane.rowIndex="1" GridPane.columnIndex="1"
  onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExit"/>
  <Hyperlink text="www.jdbc.com" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#onClickLink"/>


</GridPane>
